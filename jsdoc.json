{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.ts$",
    "excludePattern": "(node_modules|dist|tests)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "docs",
    "encoding": "utf8",
    "recurse": true,
    "template": "default"
  }
}
